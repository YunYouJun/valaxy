  "scripts": {
    "build": "rimraf dist && tsup",
    "dev": "tsup --watch"
  },
  "dependencies": {
    "@docsearch/css": "^3.1.1",
    "@docsearch/js": "^3.1.1",
    "@iconify-json/ant-design": "^1.1.3",
    "@iconify-json/simple-icons": "^1.1.19"
  },
  "devDependencies": {
    "valaxy": "workspace:*"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search",
        "@types/react",
        "react",
        "react-dom"
      ]
    }
  }
}
