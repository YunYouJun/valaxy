{
  "name": "@valaxyjs/devtools",
  "type": "module",
  "version": "0.26.13",
  "repository": {
    "url": "https://github.com/YunYouJun/valaxy"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.mjs"
    },
    "./rpc": "./rpc.d.ts",
    "./client": "./dist/client",
    "./*": "./*"
  },
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "rpc.d.ts"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "rimraf dist && pnpm run build:client && npm run build:node",
    "build:client": "vite build src/client",
    "build:node": "unbuild",
    "dev": "npm run stub && npm run dev:client",
    "dev:client": "vite dev src/client --port 5001",
    "watch:client": "vite build src/client --watch",
    "stub": "unbuild --stub",
    "prepublishOnly": "npm run build",
    "release": "bumpp && pnpm publish"
  },
  "dependencies": {
    "@rollup/pluginutils": "catalog:",
    "axios": "catalog:",
    "body-parser": "catalog:",
    "cors": "catalog:",
    "http-proxy-middleware": "catalog:",
    "js-yaml": "catalog:",
    "pathe": "catalog:",
    "sirv": "catalog:",
    "vite-dev-rpc": "catalog:"
  },
  "devDependencies": {
    "@iconify-json/ri": "catalog:build",
    "@intlify/unplugin-vue-i18n": "catalog:build",
    "@primevue/themes": "catalog:frontend",
    "@types/body-parser": "catalog:",
    "@types/splitpanes": "catalog:",
    "@types/wicg-file-system-access": "catalog:",
    "gray-matter": "catalog:",
    "primevue": "catalog:frontend",
    "splitpanes": "catalog:",
    "typescript": "catalog:build",
    "unbuild": "catalog:build",
    "vite": "catalog:build",
    "vue-i18n": "catalog:build"
  }
}
